{
  "name": "express-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:server": "DEBUG=express-sequelize node ./bin/www",
    "up:migration": "node_modules/.bin/sequelize db:migrate",
    "down:migration": "node_modules/.bin/sequelize db:migrate:undo:all",
    "up:seeders": "node_modules/.bin/sequelize db:seed:all",
    "down:seeders": "node_modules/.bin/sequelize db:seed:undo:all",
    "build:admin": "cd admin && npm run build",
    "start-dev-admin": "cd admin && npm run start",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "hbs": "^4.0.1",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.1.0",
    "serve-favicon": "~2.3.0",
    "sqlite3": "^3.0.8",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "chartist": "^0.10.1",
    "classnames": "^2.2.5",
    "material-ui": "1.0.0-beta.34",
    "material-ui-icons": "1.0.0-beta.17",
    "npm-run-all": "^4.1.1",
    "perfect-scrollbar": "^1.3.0",
    "react-swipeable-views": "^0.12.12",
    "react-chartist": "^0.13.1",
    "history": "^4.7.2",
    "react-router-dom": "^4.2.2",
    "react-google-maps":"^9.4.5",
    "sequelize-auto-migrations": "^1.0.2"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "eslint": "^4.19.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-react": "^7.7.0",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "supertest": "^2.0.0"
  }
}
