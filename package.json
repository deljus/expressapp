    "down:seeders": "sequelize db:seed:undo:all",
    "start:admin": "webpack-dev-server --config ./webpack.config.js --mode development --open",
    "build:admin": "webpack --mode production",
    "test-api": "NODE_ENV=test && mocha --compilers js:babel-core/register  --require babel-polyfill test/api/*.js "
